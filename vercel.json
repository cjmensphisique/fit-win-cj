{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://cj-fitness-backend.onrender.com/api/$1"
    },
    {
      "source": "/((?!api/|robots.txt|sitemap.xml|manifest.webmanifest).*)",
      "destination": "/index.html"
    }
  ]
}
